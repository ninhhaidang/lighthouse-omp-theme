{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#880000",
          "foreground": "#ff8888",
          "style": "plain",
          "template": "{{ if gt .Code 0 }} {{ .Code }} {{ end }}",
          "type": "status"
        },
        {
          "background": "#fc875d",
          "foreground": "#ffffff",
          "style": "plain",
          "template": "\uf0e7",
          "type": "root"
        },
        {
          "background": "#3571d1",
          "foreground": "#ffffff",
          "style": "plain",
          "template": " {{ if .WSL }}\uebcc {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "#3B82F6",
          "foreground": "#ffffff",
          "style": "plain",
          "template": " {{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "background": "#60A5FA",
          "foreground": "#000000",
          "properties": {
            "folder_icon": "\u2026",
            "style": "full",
            "folder_separator_icon": " â—† "
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#007ACC",
          "foreground": "#f1f0e9",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "style": "plain",
          "template": " {{ .HEAD }} ",
          "type": "git"
        },
        {
          "background": "#512BD4",
          "foreground": "#ffffff",
          "properties": {
            "fetch_version": false
          },
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \ue77f ",
          "type": "dotnet"
        },
        {
          "background": "#7FD5EA",
          "foreground": "#ffffff",
          "properties": {
            "fetch_version": false
          },
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \ue626 ",
          "type": "go"
        },
        {
          "background": "#FFD43B",
          "foreground": "#306998",
          "properties": {
            "fetch_version": false
          },
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \ue235 ",
          "type": "python"
        },
        {
          "background": "#CE422B",
          "foreground": "#ffffff",
          "properties": {
            "fetch_version": false
          },
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \ue7a8 ",
          "type": "rust"
        },
        {
          "background": "#68A063",
          "foreground": "#ffffff",
          "properties": {
            "fetch_version": false
          },
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \ue718 ",
          "type": "node"
        },
        {
          "background": "#3178c6",
          "foreground": "#ffffff",
          "properties": {
            "command": "tsc",
            "args": ["--version"]
          },
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \ue628 ",
          "type": "command"
        },
        {
          "background": "#ED8B00",
          "foreground": "#ffffff",
          "properties": {
            "fetch_version": false
          },
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \ue256 ",
          "type": "java"
        },
        {
          "background": "#00599c",
          "foreground": "#ffffff",
          "properties": {
            "command": "gcc",
            "args": ["--version"]
          },
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \ue61d ",
          "type": "command"
        },
        {
          "background": "#777bb3",
          "foreground": "#ffffff",
          "properties": {
            "fetch_version": false
          },
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \ue73d ",
          "type": "php"
        },
        {
          "background": "#0db7ed",
          "foreground": "#ffffff",
          "properties": {
            "fetch_version": false
          },
          "style": "plain",
          "template": " \uf308 ",
          "type": "docker"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#007ACC",
          "foreground": "#ffffff",
          "properties": {
            "style": "austin",
            "threshold": 321
          },
          "style": "plain",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "#ffffff",
          "foreground": "#007ACC",
          "style": "plain",
          "template": " \uf017 {{ .CurrentDate | date \"15:04:05\" }} ",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#007ACC",
          "style": "plain",
          "template": "{{ .Name }} ",
          "type": "shell",
          "properties": {
            "mapped_shell_names": {
              "pwsh": "\u276f",
              "shell": "\u276f",
              "cmd": ">",
              "lua": ">",
              "nu": ":)",
              "fish": "~>",
              "zsh": "%",
              "bash": "$"
            }
          }
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 3
}
