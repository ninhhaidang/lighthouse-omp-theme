{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": "❯ ",
    "foreground": "#76ff00",
    "background": "transparent",
    "foreground_templates": [
      "{{ if gt .Code 0 }}#ff0000{{ end }}"
    ]
  },
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ if .Root }}\uf0e7{{ else }}{{ if .WSL }}\ue712 {{ .Icon }}{{ else }}{{ .Icon }}{{ end }}{{ end }} ",
          "foreground": "#080808",
          "powerline_symbol": "\ue0b0",
          "background": "#DDDFE3",
          "type": "os",
          "style": "diamond",
          "foreground_templates": [
            "{{ if .Root }}#FF8C00{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_separator_icon": "\\",
            "style": "full",
            "home_icon": "~"
          },
          "template": " {{ if eq .Path \"~\" }}\uf015  {{ .Path }}{{ else }}\uf115  {{ .Path }}{{ end }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#61AFEE",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "github_icon": "\uf113 ",
            "cache_duration": "none",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " on {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#98C379",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#c4a000{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#f17c37{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "roundrock",
            "threshold": 100
          },
          "template": " took {{ .FormattedMs }} \uf252 ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b2",
          "background": "#E5C07B",
          "type": "executiontime",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .Env.CONDA_DEFAULT_ENV }} \ue73c ",
          "foreground": "#080808",
          "powerline_symbol": "\ue0b2",
          "background": "#61AFEE",
          "type": "session",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "template": " at {{ .CurrentDate | date .Format }} \uf017 ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b2",
          "background": "#DDDFE5",
          "type": "time",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": " {{ if gt .Code 0 }}\uf00d{{ else }}\uf00c{{ end }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b2",
          "background": "#98C379",
          "type": "status",
          "style": "diamond",
          "background_templates": [
            "{{ if gt .Code 0 }}#FF8F8F{{ end }}"
          ],
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "❯ ",
          "foreground": "#76ff00",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff0000{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}